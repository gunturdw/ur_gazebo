gripper_controller:
    type: effort_controllers/GripperActionController
    #type: effort_controllers/JointPositionController
    joint: gripper_joint
    action_monitor_rate: 20
    goal_tolerance: 0.002
    max_effort: 100
    stall_velocity_threshold: 0.001
    stall_timeout: 1.0
    pid: {p: 10000, i: 5, d: 100, i_clamp: 1}
